n@mixin cross-browser-support-translate-x-set {\r\n    transform:              $transform-x;\r\n    -webkit-transform:      $transform-x;\r\n    -moz-transform:         $transform-x;\r\n    -ms-transform:          $transform-x;\r\n    -o-transform:           $transform-x;\r\n}\r\n\r\n@mixin cross-browser-support-transition-set {\r\n    transition:             $transition-05;\r\n    -webkit-transition:     $transition-05;\r\n    -moz-transition:        $transition-05;\r\n    -o-transition:          $transition-05;\r\n}","scss\\partials\\_responsive.scss":"@media only screen and (max-width: 1270px) {\r\n    .container {\r\n        overflow-x: scroll;\r\n        font-size: 1.1rem;\r\n    }\r\n}\r\n\r\n@media only screen and (max-width: 600px) {\r\n    .left-col {\r\n        position: fixed;\r\n        left: 20px;\r\n    }\r\n    td {\r\n        position: relative;\r\n        left: 60px;\r\n    }\r\n}","scss\\partials\\_typography.scss":"/****** GEMUNU LIBRE - 200 - EXTRA LIGHT ******/\r\n\r\n@font-face {\r\n    font-family: Gemunu Libre;\r\n    font-weight: 200;\r\n    font-variant: Extra Light;\r\n    src:    url('../../fonts/GemunuLibre-ExtraLight.woff') format('woff'),\r\n            url('../../fonts/') format('woff2');\r\n    font-display: swap;\r\n}\r\n\r\nbody {\r\n    font-family: 'Gemunu Libre', monospace;\r\n    font-weight: 200;\r\n    color: #f5f5f5;\r\n    font-size: 32px;\r\n}","scss\\style.scss":"@import 'partials/globals',\r\n        'partials/mixins',\r\n        'partials/responsive',\r\n        'partials/typography',\r\n        'partials/variables';\r\n\r\n.container {\r\n    border-radius: 10px;\r\n    padding: 20px 30px;\r\n    width: auto;\r\n    td {\r\n        padding-inline: 40px;\r\n        &:is(.currency-heading) {\r\n            font-weight: 900;\r\n            padding-bottom: 30px;\r\n        }\r\n    }\r\n    .currency-entry {\r\n        @include cross-browser-support-transition-set;\r\n        &:hover {\r\n            border-radius: 5px;\r\n            outline: 2px solid #f5f5f5;\r\n            @include cross-browser-support-transition-set;\r\n            cursor: pointer;\r\n        }\r\n    }\r\n}\r\n\r\n#btn-container {\r\n    position: absolute;\r\n    top: 1em;\r\n    right: 1em;\r\n    width: 2.1em;\r\n    height: 1em;\r\n    border-radius: 10em;\r\n    box-shadow: inset 0 8px 60px #0000001a,\r\n                inset 0 8px 8px #0000001a,\r\n                inset 0 -4px 4px #0000001a;\r\n    .btn-toggle > .btn-icon-container {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        background-color: #f5f5f5;\r\n        width: 1em;\r\n        height: 1em;\r\n        border-radius: 50%;\r\n        cursor: pointer;\r\n        box-shadow: 0 8px 40px rgba(0, 0, 0, 0.2);\r\n        @include cross-browser-support-transition-set;\r\n    }\r\n}\r\n\r\n.dark-mode .btn-icon-container {\r\n    @include cross-browser-support-translate-x-set;\r\n    @include cross-browser-support-transition-set;\r\n}\r\n\r\n.icon-transitions {\r\n    @include cross-browser-support-transition-set;\r\n}\r\n\r\n.spin-animations {\r\n    animation: spin 0.5s;\r\n    -webkit-animation: spin 0.5s;\r\n    -moz-animation: spin 0.5s;\r\n    -o-animation: spin 0.5s;\r\n}\r\n\r\n@keyframes spin {\r\n    to {\r\n        transform: rotate(360deg);\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"272098aa4775a3906015fb925a991b89","cacheData":{"env":{}}}